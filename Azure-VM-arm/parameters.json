{
    "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentParameters.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "location": {
            "value": "centralindia"
        },
        "networkInterfaceName": {
            "value": "vm171"
        },
        "networkSecurityGroupName": {
            "value": "vm-nsg"
        },
        "networkSecurityGroupRules": {
            "value": [
                {
                    "name": "SSH",
                    "properties": {
                        "priority": 300,
                        "protocol": "TCP",
                        "access": "Allow",
                        "direction": "Inbound",
                        "sourceAddressPrefix": "*",
                        "sourcePortRange": "*",
                        "destinationAddressPrefix": "*",
                        "destinationPortRange": "22"
                    }
                }
            ]
        },
        "subnetName": {
            "value": "subnet-1"
        },
        "virtualNetworkName": {
            "value": "vnet"
        },
        "addressPrefixes": {
            "value": [
                "10.0.0.0/16"
            ]
        },
        "subnets": {
            "value": [
                {
                    "name": "subnet-1",
                    "properties": {
                        "addressPrefix": "10.0.0.0/24"
                    }
                }
            ]
        },
        "publicIpAddressName": {
            "value": "vm-ip2"
        },
        "publicIpAddressType": {
            "value": "Static"
        },
        "publicIpAddressSku": {
            "value": "Standard"
        },
        "pipDeleteOption": {
            "value": "Detach"
        },
        "virtualMachineName": {
            "value": "vm"
        },
        "virtualMachineComputerName": {
            "value": "vm2"
        },
        "virtualMachineRG": {
            "value": "vmbicep"
        },
        "osDiskType": {
            "value": "Premium_LRS"
        },
        "osDiskSizeGiB": {
            "value": 64
        },
        "osDiskDeleteOption": {
            "value": "Delete"
        },
        "virtualMachineSize": {
            "value": "Standard_B1s"
        },
        "nicDeleteOption": {
            "value": "Detach"
        },
        "adminUsername": {
            "value": "satish"
        },
        "adminPassword": {
            "value": "@Password12345"
        },
        "securityType": {
            "value": "TrustedLaunch"
        },
        "secureBoot": {
            "value": true
        },
        "vTPM": {
            "value": true
        }
    }
}